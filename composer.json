{
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "John VanDeWeghe",
            "email": "vandeweghe@lunixlabs.com"
        }
    ],
    "require": {
        "php": "^8",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/api-pack": "^1.3",
        "firebase/php-jwt": "^5.2.0",
        "doctrine/doctrine-migrations-bundle": "^2.1",
        "ramsey/uuid-doctrine": "^1.6",
        "doctrine/persistence": "^2.1",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.2.*",
        "symfony/messenger": "5.2.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/yaml": "5.2.*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "fakerphp/faker": "^1.13",
        "infection/infection": "^0.20.2",
        "phpstan/phpstan": "^0.12.64",
        "roave/security-advisories": "dev-master",
        "symfony/maker-bundle": "^1.15",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "EventStreamApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EventStreamApi\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    },
    "suggest": {
        "sroze/messenger-enqueue-transport": "To add a bunch more transport support to messanger.",
        "enqueue/gps": "To use PubSub for messanger."
    }
}
